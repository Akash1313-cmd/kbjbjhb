{
  "name": "gmap-api",
  "version": "3.0.1",
  "description": "Google Maps Scraper + REST API",
  "main": "src/api-server.js",
  "scripts": {
    "start": "node src/api-server.js",
    "start:prod": "cross-env NODE_ENV=production node --max-old-space-size=2048 --expose-gc src/api-server.js",
    "start:prod:pm2": "pm2 start ecosystem.config.js --env production",
    "start:dev": "cross-env NODE_ENV=development nodemon src/api-server.js",
    "start:memory": "node --max-old-space-size=2048 --expose-gc --trace-warnings src/api-server.js",
    "scrape": "node src/scraper-pro.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:logs": "pm2 logs",
    "pm2:monitor": "pm2 monit",
    "postinstall": "node scripts/post-install.js",
    "setup": "node scripts/setup-check.js",
    "test": "bash tests/test-complete-api.sh",
    "test:new": "bash tests/test-top5-apis.sh",
    "test:workers": "bash tests/test-worker-config.sh",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build"
  },
  "keywords": [
    "google-maps",
    "scraper",
    "api",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.3",
    "morgan": "^1.10.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^4.6.12",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "puppeteer": "^24.26.0"
  }
}
